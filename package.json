{
  "name": "vue-cesium-earth",
  "version": "0.0.0",
  "scripts": {
    "cz": "git-cz",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint ./src --ext .vue,.js,.ts,.jsx,.tsx && pretty-quick --check --branch main ./src",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm -y"
  },
  "dependencies": {
    "@quasar/extras": "^1.15.3",
    "@turf/turf": "^6.5.0",
    "axios": "^0.27.2",
    "axios-mock-adapter": "^1.20.0",
    "interactjs": "1.4.14",
    "lodash": "^4.17.21",
    "lowdb": "1.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.22",
    "qs": "^6.10.2",
    "quasar": "2.10.0",
    "screenfull": "^6.0.0",
    "uuid": "^9.0.0",
    "vue": "^3.2.39",
    "vue-cesium": "^3.1.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.5",
    "webrtc-adapter": "^8.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@commitlint/cz-commitlint": "^17.0.3",
    "@quasar/vite-plugin": "1.2.3",
    "@types/node": "18.11.3",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "commitizen": "^4.2.4",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-vue": "^9.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "pretty-quick": "3.1.3",
    "sass": "1.32.12",
    "terser": "^5.15.1",
    "typescript": "4.8.4",
    "unplugin-vue-define-options": "^0.12.0",
    "vite": "3.1.8",
    "vue-tsc": "^1.0.8"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie 11",
    "not op_mini all"
  ],
  "engines": {
    "node": ">= 14.12.1"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --cache --fix",
    "*.--write": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
