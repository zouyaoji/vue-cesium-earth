    "lowdb": "1.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.6",
    "qs": "^6.11.2",
    "quasar": "^2.12.0",
    "uuid": "9.0.0",
    "vue": "^3.3.4",
    "vue-cesium": "^3.2.1",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.4",
    "vxe-table": "^4.4.2",
