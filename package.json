{
  "name": "vue-cesium-earth",
  "version": "0.0.0",
  "scripts": {
    "cz": "czg",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint ./src --ext .vue,.js,.ts,.jsx,.tsx && pretty-quick --check --branch main ./src",
