  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie 11",
    "not op_mini all"
  ],
  "engines": {
    "node": ">= 18"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --cache --fix",
    "*.--write": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
